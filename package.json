{
  "name": "wwwscript",
  "description": "A better way too code the web with JS",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "standard | snazzy; exit 0",
    "lint:fix": "standard --fix | snazzy; exit 0",
    "build": "babel src/ -D -d ./lib; exit 0",
    "build:watch": "babel src/ -D -d ./lib --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build && npm test"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "module"
  ],
  "author": {
    "name": "poofmoyoo",
    "email": "poofmoyo@aol.com",
    "url": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/www-js/WWWScript"
  },
  "bugs": {
    "url": "https://github.com/www-js/WWWScript/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "coveralls": "^2.13.1",
    "jest": "^21.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "dependencies": {
    "wwwscript": "file:"
  }
}
